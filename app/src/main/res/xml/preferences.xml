<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <PreferenceCategory
        app:key="notifications_category"
        app:title="通知">
        <SwitchPreferenceCompat
            app:defaultValue="true"
            app:key="notifications"
            app:title="允许通知" />
        <MultiSelectListPreference
            android:defaultValue="@array/multi_select_list_preference_default_value"
            android:entries="@array/multi_select_list_preference_default_value"
            android:entryValues="@array/multi_select_list_preference_default_value"
            android:key="notifications_time"
            android:title="通知时间设置" />

        <!--notifications time setting-->
    </PreferenceCategory>
    <PreferenceCategory
        app:key="token_category"
        app:title="token设置">
        <EditTextPreference
            app:key="token"
            app:title="汇率token" />
    </PreferenceCategory>

</PreferenceScreen>